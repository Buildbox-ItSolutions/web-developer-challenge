// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Posts {
  id String @id @default(uuid())
  name String
  post String
  uri String?
  created_at DateTime @default(now())

  @@map("post")
}